{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "wt/globalblock",
	"title": "Global Block",
	"category": "webtero-blocks",
	"icon": "layout",
	"description": "Display content from a reusable global block",
	"keywords": ["global", "reusable", "template"],
	"textdomain": "webtero",
	"supports": {
		"anchor": true,
		"className": true
	},
	"attributes": {
		"webteroOptions": {
			"type": "string",
			"default": ""
		},
		"global_block_id": {
			"type": "string",
			"default": ""
		}
	},
	"example": {
		"attributes": {
			"global_block_id": ""
		}
	}
}
